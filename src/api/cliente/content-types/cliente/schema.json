{
  "kind": "collectionType",
  "collectionName": "clientes",
  "info": {
    "singularName": "cliente",
    "pluralName": "clientes",
    "displayName": "Clientes"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "addresses": {
      "type": "component",
      "component": "attributes.address-attributes",
      "repeatable": true
    },
    "ordenes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::orden.orden",
      "mappedBy": "cliente"
    }
  }
}
